{
  "version": "1.1.6",
  "name": "bella-scheduler",
  "description": "A useful helper for any javascript program",
  "homepage": "https://www.npmjs.com/package/bella-scheduler",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/bella-scheduler"
  },
  "author": "@ndaidong",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "lint": "eslint .",
    "nsp": "nsp check --output checkstyle",
    "tape": "nyc tape test/start.js | tap-spec",
    "report": "nyc report --reporter=lcov",
    "pretest": "npm run lint && npm run nsp",
    "test": "npm run tape",
    "posttest": "npm run report",
    "coveralls": "npm test && cat ./coverage/lcov.info | coveralls",
    "build": "gccmin"
  },
  "devDependencies": {
    "coveralls": "2.x.x",
    "eslint": "3.x.x",
    "eslint-config-ggc": "1.x.x",
    "gcc-min": "3.x.x",
    "nsp": "2.x.x",
    "nyc": "10.x.x",
    "sinon": "1.x.x",
    "tap-spec": "4.x.x",
    "tape": "4.x.x"
  },
  "gccmin": {
    "source": "src",
    "target": "dist",
    "filename": "scheduler",
    "globalVar": "scheduler"
  },
  "keywords": [
    "cron",
    "cronjob",
    "scheduler",
    "timer",
    "task",
    "util"
  ],
  "license": "MIT"
}
